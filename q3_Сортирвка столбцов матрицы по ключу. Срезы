function slice(A::Matrix,I::Vector{Int},J::Vector{Int})
    B=Matrix{eltype(A)}(undef,length(I),length(J))
    for i in 1:length(I)
        for j in 1:length(J)
            B[i,j]=A[I[i],J[j]]
        end
    end
    return B
end

sortkey!(A::AbstractMatrix, key_values) = A[:, sortperm(key_values)]

#=
julia> sortkey!(A,vec( sum(abs2, A, dims = 1)))
2Ã—2 Matrix{Int64}:
4 1
3 8
=#
